<!DOCTYPE html>
<html>
<head>
  <title>EmpApp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
   <%= stylesheet_link_tag    "custom" %>
  
  <%= csrf_meta_tags %>
  <% render 'layouts/shim' %>
</head>
<body>
 <div class="container">

    	<% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
 
   
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>

</body>
</html>
